    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-plugin-context": "^2.9.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.1.6",
    "typescript": "4.5.2"
  },
  "repository": {
